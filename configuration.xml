<?xml version="1.0"?>
<celsiusv2.1.configuration>
<element>
  <filetype>..none..</filetype>
  <email>thunderbird -compose "attachment='%from%'"</email>
  <proxy-address>http://proxy.maths.tcd.ie</proxy-address>
  <proxy-port>80</proxy-port>
  <proxy>false</proxy>
  <annotate>xournal %from%</annotate>
  <maxthreads1>6</maxthreads1>
  <maxthreads2>6</maxthreads2>
  <iconfolder>Icons</iconfolder>
</element>
<element>
  <filetype>pdf</filetype>
  <Extractor>pdftotext -q '%from%' '%to%'</Extractor>
  <Viewer>okular '%from%'</Viewer>
  <Ident>%PDF</Ident>
  <Secondary>Acroread:acroread '%from%'</Secondary>
</element>
<element>
  <filetype>ps.gz</filetype>
  <Extractor>ps2ascii '%from%' '%to%'</Extractor>
  <Viewer>okular '%from%'</Viewer>
  <Ident>%!PS</Ident>
</element>
<element>
  <filetype>djvu</filetype>
  <Extractor>djvused -e print-pure-txt '%from%' >%to%</Extractor>
  <Viewer>okular '%from%'</Viewer>
  <Ident></Ident>
  <Secondary>djview:djview '%from%'</Secondary>
</element>
