<?xml version="1.0"?>
<celsiusv2.2.librarytemplates>
<element>
  <name>Books/eBooks</name>
  <index>title|authors|pages|identifier|type|keywords</index>
  <tablecolumns>type|title|authors&1</tablecolumns>
  <columnheaders>|Title|Authors</columnheaders>
  <columntypes>symbol|text|people</columntypes>
  <columnsizes>-20|336|151</columnsizes>
  <autosortcolumn>1</autosortcolumn>
  <people>authors</people>
  <plugins-manual>Convert ps.gz to pdf|Create Thumbnails (epub,djvu,pdf)|CropPDF/eReader|Look at Amazon|Look at PhilSci Archive|Identify Barcode|Stamp PDF Header|Repair PDF</plugins-manual>
  <plugins-auto>Get from Filename|Get from PDF Header|Get from header|Get from arXiv|Get from Spires</plugins-auto>
  <plugins-export></plugins-export>
  <filetypes>pdf|ps.gz|djvu|epub|html|war|rtf</filetypes>
  <searchtags>title|authors|remarks|pages|type|filetype|lastmodified|date|filesize|plaintext</searchtags>
  <hide>MenuItem:Set my location to current item's location</hide>
  <essential-fields>title|authors</essential-fields>
  <differentiating-fields>title|authors</differentiating-fields>
  <item-representation>"#title#" by #authors&1#</item-representation>
  <naming-convention>#title# (#authors&1#).#filetype#</naming-convention>
  <choice-fields>type:Book,Cartoon,eBook,Lecture Notes,Manual,Newspaper Article,Other,Paper,Preprint,Review,Sheet Music,Talk,Thesis</choice-fields>
  <icon-fields>type</icon-fields>
  <item-folder>LD::documents</item-folder>
  <infoMode-0><html>
<body>
<table width='100%'>
<tr>
<td valign='top'>
<b>#authors&4#</b>
<h2>#title#</h2>
#pages# pages
#if#location#, #filesize#
#if#arxiv-name#, #arxiv-name#/#arxiv-number#
#if#date#, #date#
<br>registered in:
#if#autoregistered# |#autoregistered#|
#if#registered# |#registered#|
<br>
#if#filetype#<a href='http://$$view'>#filetype#-file</a>&nbsp;
#if#altversions# #altversions#
#if#journallink#<a href='http://$$journallink'>Journal link</a>&nbsp;
#if#bibtex.doi#<a href='http://dx.doi.org/#bibtex.doi#'>doi-link</a>&nbsp;
#if#arxiv-ref#<a href='http://www.arxiv.org/abs/#arxiv-ref#'>arXiv</a>&nbsp;
#if#spires-key#<a href='http://www.slac.stanford.edu/spires/find/hep/www?rawcmd=FIND+key+#spires-key#'>Slac-SPIRES</a>&nbsp;
#if#arxiv-ref#<a href='http://citebase.eprints.org/cgi-bin/citations?id=oai:arXiv.org:#arxiv-ref#'>CiteBase</a>&nbsp;
#if#arxiv-ref#<a href='http://eprintweb.org/S/article/#arxiv-name#/#arxiv-number#'>eprintWeb</a>&nbsp;
#if#amazon-link#<a href='#amazon-link#'>Amazon</a>&nbsp;
#if#abstract#<h3>Abstract</h3>
#if#abstract# #abstract#<br>
<br>
#if#remarks# <b>Remarks:</b> #remarks#
#if#bibtex# <h3>Bibliographic information</h3>
#if#bibtex# <pre width="20">#bibtex#</pre><br>
<h3>Celsius related information</h3>
ID: #id#
#if#recognition#, recognition level: #recognition#.
</td><td valign='top'>
#if#thumbnail#<img src='file:#$thumbnail#'>
</td></tr>
</table>
</body>
</html></infoMode-0>
  <infoMode-1><html><body>
<h2>#person#</h2>
<hr>
<br>
From this author: #currentitems# documents with #currentpages# pages in the library.<br>
<br>
#if#homepage# <a href="#homepage#">Homepage</a>&nbsp;
#if#email# #email#&nbsp;
<br><br>
<b>Collaborators:</b><br>
#collaborators#
#if#remarks# <br><br><b>Remarks:</b><br>#remarks#</body></html></infoMode-1>
  <infoMode-2><html><body>
<h2>#category#</h2>
<hr>
<br>
In this category: #currentitems# documents with #currentpages# pages in the library.<br>
</body></html></infoMode-2>
  <infoMode-3><html><body>
<h2>Multiple documents selected</h2>
<hr>
<br>
In this selection: #currentitems# documents with #currentpages# pages in the library.<br>
</body></html></infoMode-3>
  <infoMode-4><html><body>
<h2>#category#</h2>
<hr>
<br>
In this category: #currentitems# documents with #currentpages# pages in the library.<br>
</body></html></infoMode-4>
  <infoMode-5><html><body>
<h2>General Search</h2>
<hr>
<br>
In these search results: #currentitems# documents with #currentpages# pages in the library.<br>
</body></html></infoMode-5>
  <infoMode-6><html><body>
<h2>Keyword Search</h2>
<hr>
<br>
In these search results: #currentitems# documents with #currentpages# pages in the library.<br>
</body></html></infoMode-6>
  <infoMode-7><html><body>
<h2>Links</h2>
<hr>
<br>
Links of this type: #currentitems# documents with #currentpages# pages in the library.<br>
</body></html></infoMode-7>
